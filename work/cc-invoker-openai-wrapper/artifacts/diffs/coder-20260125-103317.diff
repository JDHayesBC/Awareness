index 48bca10..6e50f56 100644
--- a/pps/docker/docker-compose.yml
+++ b/pps/docker/docker-compose.yml
@@ -165,6 +165,25 @@ services:
       retries: 3
       start_period: 10s
 
+  # OpenAI-compatible wrapper for ClaudeInvoker
+  # Provides /v1/chat/completions endpoint for Graphiti to eliminate OpenAI costs
+  pps-cc-wrapper:
+    build:
+      context: ..
+      dockerfile: docker/Dockerfile.cc-wrapper
+    container_name: pps-cc-wrapper
+    restart: unless-stopped
+    ports:
+      - "127.0.0.1:8204:8000"
+    environment:
+      - WRAPPER_MODEL=${WRAPPER_MODEL:-haiku}
+    healthcheck:
+      test: ["CMD", "curl", "-f", "http://localhost:8000/health"]
+      interval: 30s
+      timeout: 10s
+      retries: 3
+      start_period: 60s  # ClaudeInvoker init takes ~33s
+
 volumes:
   chromadb_data:
     driver: local
